{
  "name": "daniloster.github.io",
  "version": "1.0.0",
  "description": "My personal blog",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha javascripts/ --recursive --glob *-spec.js --require ./test/helpers/browser.js --require ignore-styles --colors",
    "test:watch": "npm run test -- --watch",
    "bundle": "webpack --config webpack.config.js --inline --progress --colors --display-error-details --display-cached",
    "bundle:hot": "NODE_ENV=dev npm run bundle -- --watch",
    "prejekyll:live": "rm -rf public/",
    "jekyll:live": "bundle exec jekyll serve -- --watch",
    "build:prod": "NODE_ENV=production npm run bundle",
    "build:dev": "NODE_ENV=production npm run bundle",
    "build": "npm run build:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daniloster/daniloster.github.io.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "angular",
    "angularjs",
    "integration",
    "integrating",
    "javascript",
    "js"
  ],
  "author": "Danilo Castro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/daniloster/daniloster.github.io/issues"
  },
  "homepage": "https://github.com/daniloster/daniloster.github.io#readme",
